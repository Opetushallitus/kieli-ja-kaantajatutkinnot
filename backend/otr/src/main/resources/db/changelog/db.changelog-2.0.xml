        </createTable>

        <addForeignKeyConstraint baseTableName="email" baseColumnNames="email_type"
                                 referencedTableName="email_type" referencedColumnNames="name"
                                 constraintName="fk_email_email_type"
                                 onDelete="NO ACTION" onUpdate="NO ACTION"/>
